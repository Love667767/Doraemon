        <item name="android:layout_width">1px</item>
        <item name="android:background">@color/color_divider_line</item>
    </style>

    <style name="margin12_horizontal_divider" parent="common_horizontal_divider">
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

