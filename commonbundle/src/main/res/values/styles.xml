    <!-- 水平分隔线 -->
    <style name="common_horizontal_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/color_divider_line</item>
    </style>

    <!-- 竖直分隔线 -->
